{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "docker-socket",
      "host": {
        "sourcePath": "/var/run/docker.sock"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "udacity-finding-donors-server",
      "image": "quay.io/jerowe/udacity-finding-donors-server:1.4",
      "update": true,
      "essential": true,
      "memory": 248,
      "environment": [
        {
          "name": "DONOR_DATA",
          "value": "/home/flask/finding_donors_flask_app/materials/census.csv"
        }
      ],
      "mountPoints": [
      ],
      "portMappings": [
        {
          "hostPort": 5000,
          "containerPort": 5000
        }
      ],
      "links": [
        "redis"
      ]
    },
    {
      "name": "redis",
      "image": "redis:alpine",
      "update": true,
      "essential": true,
      "memory": 248,
      "mountPoints": [
      ],
      "portMappings": [
      ]
    },
    {
      "name": "udacity-finding-donors-client",
      "image": "quay.io/jerowe/udacity-finding-donors-client:1.4",
      "essential": true,
      "update": true,
      "memory": 148,
      "portMappings": [
      ],
      "links": [
        "udacity-finding-donors-server"
      ],
      "mountPoints": [
      ]
    },
    {
      "name": "traefik-manager",
      "image": "traefik:1.5-alpine",
      "essential": true,
      "update": true,
      "memory": 148,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        },
        {
          "hostPort": 443,
          "containerPort": 443
        }
      ],
      "links": [
        "udacity-finding-donors-server",
        "udacity-finding-donors-client"
      ],
      "mountPoints": [
        {
          "sourceVolume": "docker-socket",
          "containerPath": "/var/run/docker.sock",
          "readOnly": true
        }
      ]
    }
  ]
}